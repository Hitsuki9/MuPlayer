<ul style="max-height: {{ $data.listMaxLength * 32 + $data.listMaxLength - 1 + 'px' }}">
  {{ each $data.audios }}
    <li class="flex-align">
      <span class="mu-player-list-index">{{ $index + 1 }}</span>
      <span class="mu-player-list-name">{{ $value.name }}</span>
      <span class="mu-player-list-artist">{{ $value.artist }}</span>
    </li>
  {{ /each }}
</ul>
